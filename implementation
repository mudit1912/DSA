#include<bits/stdc++.h>
using namespace std;
#define ll long long int 
ll mod = 1e9+7; 
ll N = 1e6; 
void spiderman(){
int n;
cin>>n;
int b[n];
for(int i=0;i<n;i++) cin>>b[i];
 
int a[n];
for(int i=0;i<n;i++){
    a[i]=0;
}
int j=0;
for(int i=0;i<n;i=i+2){
    a[i]=b[j];
    j++;
}
int k=n-1;
for(int i=1;i<n;i=i+2){
    a[i]=b[k];
    k--;
}
for(int i=0;i<n;i++){
    cout<<a[i]<<" ";
}
cout<<endl;
}
 
 
int main(){
   int ttt;
    cin>>ttt;
    while(ttt--){
        spiderman(); 
    }
}
